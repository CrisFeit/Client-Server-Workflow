{
  "name": "full-stack-workflow",
  "version": "1.0.0",
  "description": "",
  "main": "app/server/server.ts",
  "scripts": {
    "dev": "concurrently 'npm:dev:client' 'npm:dev:server'",
    "dev:client": "webpack-dev-server --open --config webpack.dev.js",
    "dev:server": "ts-node-dev -r tsconfig-paths/register --respawn --transpileOnly --ignore-watch node_modules --no-notify app/server/server.ts",
    "build": "concurrently 'npm:build:client' 'npm:build:server'",
    "build:client": "webpack --config webpack.prod.js",
    "build:server": "babel app/server --extensions \".js,.ts\" --out-dir app/dist --copy-files --no-copy-ignored",
    "test:unit" :"jest",
    "start": "node app/server/server.ts"
  },
  "dependencies": {
    "express": "^4.17.1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-async-generator-functions": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-syntax-async-generators": "^7.8.4",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.2.0",
    "css-loader": "^3.5.3",
    "cssnano": "^4.1.10",
    "dotenv": "^8.2.0",
    "file-loader": "^6.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "sass": "^1.26.8",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2",
    "@babel/cli": "^7.10.3",
    "@babel/node": "^7.10.3",
    "@babel/preset-typescript": "^7.10.1",
    "@types/jest": "^26.0.0",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^7.3.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^26.0.1",
    "ts-jest": "^26.1.0",
    "ts-node-dev": "^1.0.0-pre.49",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.5"
  }
